<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
<bean id="address" class="beans.Address">
<property name="village" value="West Jurain"></property>
<property name="District" value="Dhaka"></property>
</bean>

<bean id="employee" class="beans.Employee">
<property name="name" value="Md. Moshiur Rahman"></property>
<property name="address" ref="address"></property>
</bean>
</beans>